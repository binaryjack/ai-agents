{
  "name": "react-multi-agent-system",
  "version": "1.0.0",
  "description": "Multi-agent system for collaborative React development",
  "main": "dist/orchestrator/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/orchestrator/index.js",
    "dev": "tsx watch src/orchestrator/index.ts",
    "start-orchestrator": "npm run build && node dist/orchestrator/index.js",
    "create-project": "npm run build && node dist/cli/create-project.js",
    "generate-component": "npm run build && node dist/cli/generate-component.js",
    "add-state": "npm run build && node dist/cli/add-state.js",
    "generate-tests": "npm run build && node dist/cli/generate-tests.js",
    "test": "jest",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ai",
    "agents",
    "development",
    "automation",
    "code-generation"
  ],
  "author": "binaryjack",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "axios": "^1.6.7",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "commander": "^12.0.0",
    "handlebars": "^4.7.8",
    "prettier": "^3.2.5",
    "eslint": "^8.57.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "@types/node": "^20.11.25"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
